#!/bin/bash

DIR="$( cd "$( dirname "${BASH_SOURCE[0]}" )" && pwd )"

filter="sat14.*\\.pddl"
zipFile="$DIR/../../../benchmarks/pddl/planning.domains.zip"

filter="seq-agl.*\\.pddl"
zipFile="$DIR/../../../benchmarks/pddl/ipc14.zip"

unzip -Z1 $zipFile | grep "$filter" | grep -v "domain[^/]*.pddl" > problemList.txt

~/bin/parallel -j 32 --shuf taskset -c {= '$_=$job->slot()-1' =} {} ::: $DIR/runSingleIncrementalExperiment ::: $zipFile :::: problemList.txt ::: MpC Srt ::: n r1 r0.5 r0.5-S5
~/bin/parallel -j 32 --shuf taskset -c {= '$_=$job->slot()-1' =} {} ::: $DIR/runSingleMadagascar ::: $zipFile :::: problemList.txt ::: S P

echo "could complete" > completeFile